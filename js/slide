$(document).ready(function(){
		$('#hide1').hide();
		$('#hide2').hide();
		$('#hide3').hide();
		
		$('.btnAbout').click(function(){
			$('#hide1').show();
		});

		$('.btnProject').click(function(){
			$('#hide2').show();
		});

		$('.btnProject').click(function(){
			$('#hide3').show();
		});
	
});
